{
  "name": "@grikomsn/v7",
  "version": "7.0.0",
  "scripts": {
    "# general commands": "##################################################",
    "build": "next build && yarn sitemap",
    "build:local": "env-cmd yarn build",
    "build:static": "yarn build && next export",
    "build:static-local": "env-cmd yarn build:static",
    "clean": "rimraf .eslintcache .next",
    "codegen": "env-cmd yarn graphql-codegen",
    "dev": "next",
    "dev:sync": "run-p -ln dev sync",
    "lint": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "precommit": "lint-staged",
    "sitemap": "next-sitemap --config config/sitemap.js",
    "sync": "browser-sync start -p \"localhost:3000\"",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check",
    "#": "###################################################################"
  },
  "dependencies": {
    "@chakra-ui/react": "^1",
    "@chakra-ui/theme-tools": "^1",
    "@chakra-ui/utils": "^1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fec/remark-a11y-emoji": "^3",
    "@grikomsn/shared": "*",
    "@types/node": "^14",
    "@types/nprogress": "^0",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-syntax-highlighter": "^13",
    "date-fns": "^2",
    "eslint": "^7",
    "eslint-config-next": "^11",
    "framer-motion": "^3",
    "graphql": "^15",
    "graphql-request": "^3",
    "graphql-tag": "^2",
    "next": "^11",
    "next-connect": "^0",
    "next-seo": "^4",
    "next-sitemap": "^1",
    "nprogress": "^0",
    "react": "^17",
    "react-datocms": "^1",
    "react-dom": "^17",
    "react-icons": "^4",
    "react-markdown": "^6",
    "react-syntax-highlighter": "^15",
    "remark-slug": "^6",
    "remark-toc": "^7",
    "tinykeys": "1.1.1",
    "typescript": "^4",
    "zustand": "^3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2",
    "@graphql-codegen/cli": "^1",
    "@graphql-codegen/schema-ast": "^1",
    "@graphql-codegen/typescript": "^1",
    "@graphql-codegen/typescript-graphql-request": "^3",
    "@graphql-codegen/typescript-operations": "^1"
  },
  "private": true
}
