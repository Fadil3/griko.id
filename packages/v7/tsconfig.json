{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "incremental": true,
    "paths": {
      "~*": ["./*"]
    }
  },
  "exclude": ["generated", "node_modules"],
  "include": [
    "env.d.ts",
    "next-env.d.ts",
    "**/*.d.ts",
    "**/*.js",
    "**/*.json",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.ts",
    "**/*.tsx"
  ]
}
